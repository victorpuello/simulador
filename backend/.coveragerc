[run]
source = apps
omit = 
    */migrations/*
    */tests/*
    */test_*.py
    */__init__.py
    */settings/*
    */venv/*
    */virtualenv/*
    manage.py
    */wsgi.py
    */asgi.py
    */urls.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    def __str__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if settings.DEBUG:
    if TYPE_CHECKING:

[html]
directory = htmlcov
title = Simulador Saber 11 - Backend Coverage Report

[xml]
output = coverage.xml